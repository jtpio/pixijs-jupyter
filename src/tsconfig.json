{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": true,
    "lib": ["dom", "es5", "es2015.promise", "es2015.iterable"],
    "noEmitOnError": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "outDir": "../lib",
    "skipLibCheck": true,
    "sourceMap": true
  }
}
